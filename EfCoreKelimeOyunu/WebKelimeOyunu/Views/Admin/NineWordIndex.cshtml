@model NineWordViewModel

@{
    ViewData["Title"] = "Index-Nine";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Nine</h1>

<div class="row">
    <div class="col-md-4">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <h2>9 HARFLİ CEVAPLAR</h2>s
        <table>
            <tr>
                <th>ID</th>
                <th>SORU</th>
                <th>CEVAP</th>
                <th>DATA</th>
                <th>PUAN</th>
            </tr>
            @foreach (var item in Model.NineWords)
            {
                <tr>
                    <td>@item.NineWordID</td>
                    <td>@item.NineWordQuestion</td>
                    <td>@item.NineWordAnswer</td>
                    <td>@item.NineWordData</td>
                    <td>@item.NineWordScore</td>
                </tr>
            }
        </table>
    </div>
</div>

<div>
    <a asp-action="Index">Anasayfa</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
