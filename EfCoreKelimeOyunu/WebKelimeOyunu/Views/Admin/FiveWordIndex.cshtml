@model FiveWordViewModel

@{
    ViewData["Title"] = "Index-Five";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Five</h1>

<div class="row">
    <div class="col-md-4">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <h2>5 HARFLİ CEVAPLAR</h2>s
        <table>
            <tr>
                <th>ID</th>
                <th>SORU</th>
                <th>CEVAP</th>
                <th>DATA</th>
                <th>PUAN</th>
            </tr>
            @foreach (var item in Model.FifthWords)
            {

                <tr>
                    <td>@item.FifthWordID</td>
                    <td>@item.FifthWordQuestion</td>
                    <td>@item.FifthWordAnswer</td>
                    <td>@item.FifthWordData</td>
                    <td>@item.FifthWordScore</td>
                </tr>
            }
        </table>
    </div>
</div>

<div>
    <a asp-action="Index">Anasayfa</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
